#include <stdio.h>

int main() {
    int n, i, j;
    float marks[50], temp;

    printf("Enter number of students: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter marks of student %d: ", i + 1);
        scanf("%f", &marks[i]);
    }

    for (i = 0; i < n - 1; i++) {
        for (j = i + 1; j < n; j++) {
            if (marks[i] > marks[j]) {
                temp = marks[i];
                marks[i] = marks[j];
                marks[j] = temp;
            }
        }
    }

    printf("Sorted Marks:\n");
    for (i = 0; i < n; i++) {
        printf("%.2f ", marks[i]);
    }

    return 0;
}
output:
Enter number of students: 5
Enter marks: 78 65 90 82 70
Sorted Marks:
65.00 70.00 78.00 82.00 90.00

