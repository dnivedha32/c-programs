#include <stdio.h>

int main() {
    char str[100], newStr[100];
    int i, j = 0;

    printf("Enter a string: ");
    scanf(" %[^\n]", str);

    for (i = 0; str[i] != '\0'; i++) {
        if (str[i] != ' ') {
            newStr[j++] = str[i];
        }
    }
    newStr[j] = '\0';

    printf("String without spaces: %s", newStr);

    return 0;
}
output:
Enter a string: Hello World
String without spaces: HelloWorld
