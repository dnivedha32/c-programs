#include <stdio.h>

float convertTemp(float c) {
    return (c * 9 / 5) + 32;
}

int main() {
    float celsius;

    printf("Enter temperature in Celsius: ");
    scanf("%f", &celsius);

    printf("Temperature in Fahrenheit = %.2f", convertTemp(celsius));

    return 0;
}
output:
Enter temperature in Celsius: 25
Temperature in Fahrenheit = 77.00
