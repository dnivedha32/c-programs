#include <stdio.h>

struct student {
    int roll;
    char name[20];
};

int main() {
    struct student s[10];
    int n, i;

    printf("Enter number of students: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        printf("Enter roll and name of student %d: ", i + 1);
        scanf("%d %s", &s[i].roll, s[i].name);
    }

    // Adding 2 new students
    s[n].roll = 106;
    strcpy(s[n].name, "Kavya");

    s[n+1].roll = 107;
    strcpy(s[n+1].name, "Suresh");

    n = n + 2; // Updated count

    printf("\nUpdated Student List:\n");
    for(i = 0; i < n; i++) {
        printf("%d %s\n", s[i].roll, s[i].name);
    }

    return 0;
}
O/P:
Enter number of students: 3
Enter roll and name of student 1: 101 Ravi
Enter roll and name of student 2: 102 Anita
Enter roll and name of student 3: 103 Kumar

Updated Student List:
101 Ravi
102 Anita
103 Kumar
106 Kavya
107 Suresh
